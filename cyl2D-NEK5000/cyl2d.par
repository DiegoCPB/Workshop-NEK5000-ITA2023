#
# nek parameter file
#
[GENERAL]
endTime  = 200
stopAt = endTime
variableDT = yes
targetCFL = 0.5
writeControl = runTime
writeInterval = 1
dealiasing = yes
timeStepper = bdf2

[PROBLEMTYPE]
equation = incompNS

[PRESSURE]
residualTol = 1e-5
residualProj = yes

[VELOCITY]
residualTol = 1e-8
density = 1
viscosity = -100 # Quando negativo, indica o numero de reynolds