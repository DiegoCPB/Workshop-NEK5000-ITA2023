#
# nek parameter file
#
[GENERAL]
#startFrom = cyl2d0.f01000 
numSteps = 5000
stopAt = numSteps
variableDT = yes
targetCFL = 0.5
writeControl = timeStep
writeInterval = 5
dealiasing = yes
timeStepper = bdf2

[PROBLEMTYPE]
equation = incompNS

[PRESSURE]
residualTol = 1e-8
residualProj = yes

[VELOCITY]
density = 1
viscosity = -100 # Quando negativo, indica o numero de reynolds